page {
  height: 100%;
  background: #f5f5f5;
}

.add-user {
  background: #fff;
}

.add-user .add-form {
  width: 100%;
  z-index: 1;
  padding-left: 30rpx;
  font-size: 0;
  border-bottom: 1px solid #d9d9d9;
}

.add-user .form-item {
  border-bottom: 1px solid #d9d9d9;
  display: inline-block;
  align-items: center;
  padding: 20rpx 30rpx;
  padding-left: 0;
  width: 100%;
}

.add-user .form-item:last-child {
  border-bottom: none;
}

.form-item .radio {
  margin-right: 20rpx;
}

.form-item .ttext {
  float: left;
  display: block;
  width: 190rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.form-item .ttext text {
  color: red;
  line-height: 60rpx;
  margin-right: 10rpx;
}

.form-item .img {
  width: 230rpx;
  height: 300rpx;
  border-radius: 10rpx;
  margin: 0 auto;
}

.form-item .imgtext {
  text-align: center;
  width: 100%;
  height: 50rpx;
  clear: both;
}

.form-item .remark {
  text-align: left;
  width: 100%;
  font-size: 28rpx;
  color: #959695;
}

.form-item .inputright {
  float: right;
  width: 500rpx;
  height: 60rpx;
  display: block;
  text-align: right;
}

.add-user .input_n {
  flex: 1;
  width: 650rpx;
  height: 200rpx;
  line-height: 44rpx;
  padding: 20rpx;
  overflow: hidden;
  text-align: left;
  border: solid 1rpx #d9d9d9;
}

.add-user .form-default {
  border-bottom: 1px solid #d9d9d9;
  height: 96rpx;
  background: #fafafa;
  padding-top: 28rpx;
  font-size: 28rpx;
}

.profile {
  width: 100%;
  padding: 20rpx;
  display: inline-block;
  margin-top: 30rpx;
}

.profile .leftimg {
  width: 70rpx;
  height: 70rpx;
  float: left;
  border-radius: 10rpx;
}

.profile .rightinfo {
  float: right;
  width: 620rpx;
  padding-left: 10rpx;
}

.profile .rightinfo .k {
  margin-top: 10rpx;
  background-color: #fff;
  padding: 20rpx;
  display: inline-block;
  width: 100%;
  border-radius: 8rpx;
  position: relative;
}

.profile .rightinfo .k::after {
  content: '';
  border: 20rpx solid #ffffff00;
  border-right: 20rpx solid #fff;
  position: absolute;
  top: 10rpx;
  left: -32rpx;
}

.profile .rightinfo .desc {
  line-height: 50rpx;
}

.btns {
  overflow: hidden;
  display: flex;
  height: 130rpx;
  width: 100%;
  padding-top: 30rpx;
  background: #f5f5f5;
}

.next,
.save {
  width: 710rpx;
  margin: 0 auto;
  display: block;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #52a936;
  border: none;
  border-radius: 10rpx;
}

.next {
  background: #fff;
  border: solid 2rpx #52a936;
}

.save {
  background: #fff;
  border: solid 2rpx #52a936;
}

.btnarea {
  width: 100%;
  padding: 20rpx;
}

.btnarea button,
.btnarea navigator {
  width: 49%;
  border: solid 2rpx #52a936;
  background-color: #fff;
  color: #52a936;
  font-size: 28rpx;
  line-height: 90rpx;
  text-align: center;
  border-radius: 10rpx;
}

.btnarea .left {
  float: left;
}

.btnarea .right {
  float: right;
}

.tis {
  padding: 20rpx;
}

.tis .desc {
  padding: 20rpx;
  line-height: 50rpx;
  background: #fff;
}

.tis .successIcon {
  margin: 0 auto;
  width: 300rpx;
  height: 300rpx;
  display: block;
  margin-bottom: 20rpx;
}

.order_process {
  display: flex;
  flex-wrap: nowrap;
  padding: 20rpx;
  background-color: #fff;
}

.process_wrap {
  display: flex;
  flex-direction: column;
  flex: 1;
  align-items: center;
}

.process {
  display: flex;
  align-items: center;
  width: 100%;
}

.process_icon {
  width: 55rpx;
  height: 55rpx;
  text-align: center;
  line-height: 50rpx;
  background: #959695;
  color: #fff;
  border-radius: 55rpx;
  border: 5rpx solid #d9d9d9;
}

.process_line {
  background: #eff3f6;
  flex: 1;
  height: 5rpx;
}

.process_icon_action {
  width: 55rpx;
  height: 55rpx;
  text-align: center;
  line-height: 50rpx;
  background: #52a936;
  color: #fff;
  border-radius: 55rpx;
  border: 5rpx solid #d9d9d9;
}

.process_line {
  background: #eff3f6;
  flex: 1;
  height: 5rpx;
}

.process_line_start {
  background: #fff;
}

.process_line_end {
  background: #52a936;
}

.process_name {
  font-size: 26rpx;
  margin-top: 10rpx;
}

.radio_group {
  float: right;
  width: 500rpx;
}

.radio_group .item {
  display: inline-block;
  text-align: center;
  margin-right: 10rpx;
}

.radio_group .item:last-child {
  margin-right: 0;
}

.radio_group .item image {
  display: block;
  width: 150rpx;
  height: 200rpx;
  border-radius: 10rpx;
}

.radio_group .item radio {
  margin-top: 20rpx;
}

.radio_group .item radio .wx-radio-input {
  width: 30rpx;
  height: 30rpx;
}