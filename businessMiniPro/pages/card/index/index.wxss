.lricon {
  position: absolute;
  top: 50%;
  margin-top: -25rpx;
  z-index: 999;
  width: 50rpx;
  height: 50rpx;
  animation: fade 1000ms infinite;
}

.lefticon {
  left: 10rpx;
}

.righticon {
  right: 10rpx;
}

@keyframes fade {
  from {
    opacity: 1.0;
  }

  50% {
    opacity: 0.5;
  }

  to {
    opacity: 1.0;
  }
}

@-webkit-keyframes fade {
  from {
    opacity: 1.0;
  }

  50% {
    opacity: 0.5;
  }

  to {
    opacity: 1.0;
  }
}











@-webkit-keyframes rightflip {

  0%,
  30% {
    opacity: 0;
    -webkit-transform: translate(10)
  }

  60% {
    opacity: 1;
    -webkit-transform: translate(0)
  }

  to {
    opacity: 0;
    -webkit-transform: translate(-8px)
  }
}

@keyframes rightflip {

  0%,
  30% {
    opacity: 0;
    transform: translate(10)
  }

  60% {
    opacity: 1;
    transform: translate(0)
  }

  to {
    opacity: 0;
    transform: translate(-8px)
  }
}

@-webkit-keyframes leftflip {

  0%,
  30% {
    opacity: 0;
    -webkit-transform: translate(-8)
  }

  60% {
    opacity: 1;
    -webkit-transform: translate(0)
  }

  to {
    opacity: 0;
    -webkit-transform: translate(10px)
  }
}

@keyframes leftflip {

  0%,
  30% {
    opacity: 0;
    transform: translate(-8)
  }

  60% {
    opacity: 1;
    transform: translate(0)
  }

  to {
    opacity: 0;
    transform: translate(10px)
  }
}

.u-arrow-left {
  position: absolute;
  width: 14px;
  height: 24px;
  z-index: 150;
  top: 50%;
  margin-top: -10px;
  left: 20px;
  transform: scaleX(-1);
  -webkit-transform: scaleX(-1);
}

.pre-wrap-left {
  width: 14px;
  height: 24px;
  position: relative;
  -webkit-animation: leftflip 1.5s infinite ease-in-out;
  animation: leftflip 1.5s infinite ease-in-out;
  text-shadow: none !important;
}

.pre-box1,
.pre-box2 {
  height: 11px;
  width: 15px;
  position: absolute;
  overflow: hidden;
}

.pre-box1 {
  top: 10px;
}

.pre1,
.pre2 {
  background-color: #ccc;
  width: 14px;
  height: 5px;
  border-radius: 2px;
  position: absolute;
}

.pre1 {
  top: 1px;
  box-shadow: 1px 1px 1.5px #646464;
  transform: rotate(40deg);
}

.pre2 {
  top: 5px;
  box-shadow: 1px -1px 2px #646464;
  transform: rotate(130deg)
}

.u-arrow-right {
  position: absolute;
  width: 14px;
  height: 24px;
  z-index: 150;
  top: 50%;
  margin-top: -10px;
  right: 20px;
  transform: scaleX(-1);
  -webkit-transform: scaleX(-1);
}

.pre-wrap-right {
  width: 14px;
  height: 24px;
  position: relative;
  -webkit-animation: rightflip 1.5s infinite ease-in-out;
  animation: rightflip 1.5s infinite ease-in-out;
  text-shadow: none !important;
}

.pre-box3,
.pre-box4 {
  height: 11px;
  width: 15px;
  position: absolute;
  overflow: hidden;
}

.pre-box3 {
  top: 10px;
}

.pre3,
.pre4 {
  background-color: #ccc;
  width: 14px;
  height: 5px;
  border-radius: 2px;
  position: absolute;
}

.pre3 {
  top: 5px;
  transform: rotate(130deg);
  -webkit-transform: rotate(130deg);
  top: 1px;
  box-shadow: 1px -1px 2px #646464;
}

.pre4 {
  top: 5px;
  top: 6px;
  box-shadow: 1px 1px 1.5px #646464;
  transform: rotate(40deg);
  -webkit-transform: rotate(40deg);
}




