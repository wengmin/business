.card {
  position: relative;
  background-color: #fff;
  height: 1136rpx;
  width: 700rpx;
  border-radius: 20rpx;
  border-right: solid 1rpx rgb(200, 200, 200);
  border-bottom: solid 1rpx rgb(200, 200, 200);
  box-shadow: 7rpx 7rpx 10rpx 7rpx rgba(0, 0, 0, 0.5);
}

.card .topimage {
  position: absolute;
  top: 130rpx;
  text-align: center;
  width: 350rpx;
  height: 350rpx;
  border-radius: 10rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
}

.card .imgremark {
  position: absolute;
  top: 0;
  text-align: center;
  background: rgb(200, 200, 200, 0.3);
  font-weight: bold;
  color: #000;
  padding: 20rpx 0;
  width: 100%;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

.card .middle {
  width: 100%;
  /* height: 100rpx; */
  position: absolute;
  top: 50%;
  margin: 0 auto;
  /* display: flex; */
  padding-left: 100rpx;
  /* margin-top: -50rpx; */
}

.card .middle .name {
  font-size: 90rpx;
  line-height: 100rpx;
  font-weight: 500;
  clear: both;
}

.card .middle .text {
  clear: both;
  display: block;
  font-size: 32rpx;
  margin-top: 10rpx;
}

.card .middle .right {
  margin-left: 30rpx;
  text-align: left;
  line-height: 50rpx;
}

.card .middle .right text {
  display: block;
  font-size: 32rpx;
}

.card .bottom {
  width: 100%;
  text-align: center;
  position: absolute;
  bottom: 0;
}

.card .bottom .item {
  height: 50rpx;
  line-height: 50rpx;
  padding-left: 100rpx;
  display: flex;
  text-align: left;
}

.card .bottom .item .key {
  width: 100rpx;
  height: 50rpx;
  font-size: 32rpx;
}

.card .bottom .item .value {
  width: 450rpx;
  line-height: 50rpx;
  margin-left: 10rpx;
  font-size: 32rpx;
}

.card .bottom .comname {
  display: inline-block;
  padding: 20rpx 0;
  width: 100%;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  background: rgb(200, 200, 200, 0.3);
  font-weight: bold;
  color: #000;
  clear: both;
  margin-top: 100rpx;
  font-size: 32rpx;
}

